{
  "Dockerfile": "FROM python:3.9

WORKDIR /app

COPY requirements.txt ./

RUN pip install --no-cache-dir -r requirements.txt

COPY . .

CMD [\"python\", \"app.py\"]",
  "docker-compose.yml": "version: '3'
services:
  web:
    build: .
    ports:
      - '5000:5000'
    volumes:
      - .:/app
    environment:
      FLASK_APP: app.py
      FLASK_ENV: development"
}
