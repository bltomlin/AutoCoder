{
  "Dockerfile": "FROM python:3.8

WORKDIR /app

COPY . .

RUN pip install -r requirements.txt

CMD [\"python\", \"app.py\"]",
  "docker-compose.yml": "version: '3'

services:
  app:
    build: .
    ports:
      - '5000:5000'
    environment:
      - DEBUG=True
    volumes:
      - ./app:/app
    depends_on:
      - db
  db:
    image: postgres:latest
    environment:
      - POSTGRES_USER=myuser
      - POSTGRES_PASSWORD=mypass
      - POSTGRES_DB=mydb"
}
